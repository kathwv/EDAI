#inicializando variables 
x = 10 
print(x)

cadena = "Hola mundo"
print(cadena)

#asigna un mismo valor a tres variables
x = y = z = 10
print(x, y, z)

#type() permite conocer tipo de variable
type(x)
type(cadena)

x = "Hola mundo"
cadena = 10

type(x)
type(cadena)

